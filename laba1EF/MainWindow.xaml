<Window x:Class="laba1EF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:laba1EF"
        mc:Ignorable="d"
        Title="DevelopersEF" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="83*"/>
            <RowDefinition Height="351*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="DevelopersButton" Content="Developers Table" Grid.Column="0" Click="DevelopersButton_Click"/>
        <Button x:Name="GamesButton" Content="Games Table" Grid.Column="1" Click="GamesButton_Click"/>
        <Button x:Name="PlatformsButton" Content="Platforms Table" Grid.Column="2" Click="PlatformsButton_Click"/>
        <DataGrid x:Name="MyDataGrid" Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="1" ></DataGrid>
        <Frame x:Name="MyFrame"/>
    </Grid>
</Window>
